# Sum of  two integers entered from user

.text
# main prgram
main:
li $v0, 5			# read integer
syscall

add $a0, $v0, $zero	# $a0 = $v0
li $v0, 5			#read integer
syscall

add $a0, $a0, $v0	# sum of integers ($v0 + $a0)
li $v0, 1			# print integer $a0
syscall		
	
li $v0, 10 			# exist program
syscall

# END